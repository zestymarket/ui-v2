{
  "name": "zesty-ui-v2",
  "description": "A ui for Zesty Market P2P Market Dapp",
  "version": "1.0.0",
  "private": true,
  "author": "Zesty Market",
  "license": "AGPL-3.0",
  "keywords": [
    "nextjs",
    "market",
    "p2p",
    "dapp"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
